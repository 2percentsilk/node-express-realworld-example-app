{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch (Nodemon)",
            "runtimeExecutable": "nodemon",
            "restart": true,
            "skipFiles": [
                "<node_internals>/**/*.js"
            ],
            "env": {
                "NODE_ENV": "debug"
            },
            "console": "integratedTerminal"
        }
    ]
}